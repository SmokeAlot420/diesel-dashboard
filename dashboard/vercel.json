{
  "version": 2,
  "name": "diesel-dashboard",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "@api_base_url",
    "VITE_WS_URL": "@ws_url",
    "VITE_ENABLE_MAINNET": "@enable_mainnet",
    "VITE_ENABLE_WEBSOCKET": "@enable_websocket",
    "VITE_ENABLE_ALERTS": "@enable_alerts",
    "VITE_ALERT_THRESHOLD": "@alert_threshold"
  },
  "regions": ["iad1"]
}